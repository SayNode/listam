{
  "expo": {
    "name": "listam",
    "slug": "listam-expo",
    "version": "1.0.0",
    "newArchEnabled": true,
    "orientation": "portrait",
    "scheme": "ch.saynode.listam",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ch.saynode.listam",
      "appleTeamId": "4F2NJTPHF4",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan loyalty card barcodes and QR codes."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "ch.saynode.listam"
    },
    "plugins": [
      [
        "expo-router"
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 29
          }
        }
      ],
      "react-native-iap",
      [
        "expo-camera",
        {
          "cameraPermission": "This app uses the camera to scan loyalty card barcodes and QR codes."
        }
      ]
    ]
  }
}
